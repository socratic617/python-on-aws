{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "s3:Describe*",
                "s3:Get*",
                "s3:List*",
                "s3:DeleteObject*",
                "s3:PutObject"
            ],
            "Resource": "arn:aws:s3:::cloud-course-bucket-eric/*"
        },
        {
            "Effect": "Allow",
            "Action": "s3:ListBucket",
            "Resource": "arn:aws:s3:::cloud-course-bucket-eric"
        }
    ]
}