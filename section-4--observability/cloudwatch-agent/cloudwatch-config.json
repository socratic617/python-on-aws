{
  "agent": {
    "metrics_collection_interval": 5,
    "region": "us-west-2",
    "logfile": "/opt/aws/amazon-cloudwatch-agent/logs/amazon-cloudwatch-agent.log",
    "debug": true,
    "omit_hostname": true,
    "run_as_user": "root",
    "usage_data": false
  },
  "logs": {
    "force_flush_interval" : 15,
    "logs_collected": {
      "files": {
        "collect_list": [
          {
            "file_path": "/var/log/fastapi.all.log/buffer.b61f1ca173bf102a4d4aebfa6d2e0a8d2.log",            
            "log_group_name": "local-fastapi",
            "log_stream_name": "local-fastapi-stream",
            "timestamp_format": "%Y-%m-%d %H:%M:%S"
          }
        ]
      }
    }
  },
  "traces": {
    "traces_collected": {
      "xray": {
        "bind_address": "0.0.0.0:2000",
        "tcp_proxy": {
          "bind_address": "0.0.0.0:2000"
        }
      }
    }
  }
}
